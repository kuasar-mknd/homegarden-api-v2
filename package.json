{
  "name": "homegarden-api",
  "version": "2.0.0",
  "description": "HomeGarden Plant Management API with AI capabilities - Clean Architecture",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "tsx watch index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "lint": "eslint . --ext .ts --ignore-pattern dist --ignore-pattern node_modules",
    "format": "prettier --write \"**/*.ts\" --ignore-path .gitignore",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "plant-management",
    "garden",
    "ai",
    "hono",
    "prisma",
    "typescript",
    "clean-architecture"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@hono/node-server": "^1.19.7",
    "@hono/zod-validator": "^0.7.6",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^6.19.1",
    "@supabase/supabase-js": "^2.89.0",
    "@types/pg": "^8.16.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "hono": "^4.11.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "prisma": "^7.2.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
