{
  "name": "homegarden-api",
  "version": "2.0.0",
  "description": "HomeGarden Plant Management API with AI capabilities - Clean Architecture",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio",
    "lint": "biome lint .",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "ci:check": "biome ci .",
    "test": "vitest --no-file-parallelism",
    "test:coverage": "vitest run --coverage --no-file-parallelism",
    "test:e2e": "vitest run --no-file-parallelism tests/e2e"
  },
  "keywords": [
    "plant-management",
    "garden",
    "ai",
    "hono",
    "prisma",
    "typescript",
    "clean-architecture"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@hono/node-server": "^1.19.7",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.6",
    "@hono/zod-validator": "^0.7.6",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@supabase/supabase-js": "^2.89.0",
    "@types/pg": "^8.16.0",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "hono": "^4.11.1",
    "hono-rate-limiter": "^0.5.1",
    "jsonwebtoken": "^9.0.3",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "prisma": "^7.2.0",
    "ws": "^8.18.3",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@faker-js/faker": "^10.1.0",
    "@hono/vite-dev-server": "^0.23.0",
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "@types/supertest": "^6.0.3",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-istanbul": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "pino-pretty": "^13.1.3",
    "supertest": "^7.1.4",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  }
}
